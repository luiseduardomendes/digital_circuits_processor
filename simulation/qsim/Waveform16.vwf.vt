// expected t[ 0 ]
initial
begin
	t_expected[0] = 1'bX;
end 
// generate trigger
always @(t_expected or t)
begin
	trigger <= ~trigger;
end

always @(posedge sampler_rx or negedge sampler_rx)
begin
`ifdef debug_tbench
	$display("Scanning pattern %d @time = %t",i,$realtime );
	i = i + 1;
	$display("| expected t = %b | ",t_expected_prev);
