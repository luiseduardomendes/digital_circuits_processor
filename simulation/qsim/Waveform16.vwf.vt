`timescale 1 ps/ 1 ps
module testes_vlg_sample_tst(
	Ck,
	Rst,
	sampler_tx
);
input  Ck;
input  Rst;
output sampler_tx;

reg sample;
time current_time;
always @(Ck or Rst)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

module testes_vlg_check_tst (
	t,
	sampler_rx
);
input [7:0] t;
input sampler_rx;

reg [7:0] t_expected;

reg [7:0] t_prev;

reg [7:0] t_expected_prev;

reg [7:0] last_t_exp;

reg trigger;

integer i;
integer nummismatches;

reg [1:1] on_first_change ;


initial
begin
trigger = 0;
i = 0;
nummismatches = 0;
on_first_change = 1'b1;
end

// update real /o prevs

always @(trigger)
begin
	t_prev = t;
end

// update expected /o prevs

always @(trigger)
begin
	t_expected_prev = t_expected;
end


// expected t[ 7 ]
initial
begin
	t_expected[7] = 1'bX;
end 
// expected t[ 6 ]
initial
begin
