module testes_vlg_vec_tst();
// constants                                           
// general purpose registers
reg Ck;
reg Rst;
// wires                                               
wire [7:0] t;

wire sampler;                             

// assign statements (if any)                          
testes i1 (
// port map - connection between master ports and signals/registers   
	.Ck(Ck),
	.Rst(Rst),
	.t(t)
);

// Ck
initial
begin
	Ck = 1'b0;
	Ck = #15000 1'b1;
	# 5000;
	repeat(98)
	begin
		Ck = 1'b0;
		Ck = #5000 1'b1;
		# 5000;
	end
end 

// Rst
initial
begin
	Rst = 1'b1;
	Rst = #10000 1'b0;
end 

testes_vlg_sample_tst tb_sample (
	.Ck(Ck),
	.Rst(Rst),
	.sampler_tx(sampler)
);

testes_vlg_check_tst tb_out(
	.t(t),
	.sampler_rx(sampler)
);
endmodule

