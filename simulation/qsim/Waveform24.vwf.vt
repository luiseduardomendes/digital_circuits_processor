testes_vlg_check_tst tb_out(
	.cg_ac(cg_ac),
	.cg_b(cg_b),
	.cg_c(cg_c),
	.cg_nz(cg_nz),
	.cg_pc(cg_pc),
	.cg_rdm(cg_rdm),
