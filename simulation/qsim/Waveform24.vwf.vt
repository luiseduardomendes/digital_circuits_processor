`timescale 1 ps/ 1 ps
module testes_vlg_sample_tst(
	b,
	c,
	clock1,
	instr,
	n,
	reset,
	v,
	z,
	sampler_tx
);
input  b;
input  c;
input  clock1;
input [7:0] instr;
input  n;
input  reset;
input  v;
input  z;
output sampler_tx;

reg sample;
time current_time;
always @(b or c or clock1 or instr or n or reset or v or z)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

